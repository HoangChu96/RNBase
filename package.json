{
  "name": "RNBase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "gen:types": "graphql-codegen -r dotenv/config && prettier-eslint --write \"src/services/graphql/graphql-sdk-generated.ts\" && npm run gen:types:fix:error",
    "gen:types:fix:error": "node scripts/fix-generated-file.js",
    "test:detox": "detox test -c ios.sim.debug",
    "build:detox": "detox build -c ios.sim.debug",
    "ci:test:detox": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:detox": "detox build -c ios.sim.release"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^9.3.6",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@tanstack/react-query": "^4.14.3",
    "apollo-upload-client": "^17.0.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-subscriptions-client": "^0.16.4",
    "i18n-js": "^4.1.1",
    "lodash.memoize": "^4.1.2",
    "react": "18.1.0",
    "react-hook-form": "^7.39.1",
    "react-native": "0.70.4",
    "react-native-app-auth": "^6.4.3",
    "react-native-appstate-hook": "^1.0.6",
    "react-native-config": "^1.4.11",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-fbsdk-next": "^11.1.0",
    "react-native-localize": "^2.2.4",
    "react-native-reanimated": "^2.12.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-query": "^4.0.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/i18n-js": "^3.8.3",
    "@types/jest": "^26.0.23",
    "@types/lodash.memoize": "^4.1.7",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^19.12.6",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "react-test-renderer": "18.1.0",
    "replace-in-file": "^6.3.5",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
